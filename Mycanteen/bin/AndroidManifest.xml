    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Workerlogin"
            android:label="@string/title_activity_workerlogin" >
        </activity>
        <activity
            android:name=".Studentchoose"
            android:label="@string/title_activity_studentchoose" >
        </activity>
        <activity
            android:name=".CanteenpageFragment"
            android:label="@string/title_activity_canteenpage_fragment" >
        </activity>
        <activity
            android:name=".CanteenlistFragment"
            android:label="@string/title_activity_canteenlist_fragment" >
        </activity>
        <activity
            android:name=".Bookcanteen"
            android:label="@string/title_activity_bookcanteen" >
        </activity>
    </application>

</manifest>
